{
    "name": "Search EPP Extensions",
    "description": "Search Extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_icon": "google_search_icon.png"
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  }